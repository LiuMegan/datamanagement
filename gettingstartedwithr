Airbnb.df <- read.csv("Airbnb NYC 2019.csv", header = TRUE) # load data. 
dim(Airbnb.df) # find number of obs and variables
head(Airbnb.df) #first 6 rows
Airbnb.df[1:10,1] #first 10 rows in 1st column
Airbnb.df[1:10, ] #first 10 rows in all column
Airbnb.df[5] # all rows in 5th column
Airbnb.df[,1] #all rows in first column
Airbnb.df[1] #all rows in first column
length(Airbnb.df) # find length of data 
data.frame(names(Airbnb.df)) #get the names of the variables in a list
airbnb2.df=Airbnb.df[50:70,] #get a sample of rows 50 to 70
summary(Airbnb.df) #get summary statistics of the data set
